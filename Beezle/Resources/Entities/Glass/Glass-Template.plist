<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>components</key>
	<dict>
		<key>crumble</key>
		<dict/>
		<key>disposable</key>
		<dict>
			<key>deleteEntityWhenDisposed</key>
			<true/>
		</dict>
		<key>physics</key>
		<dict>
			<key>file</key>
			<string>%physicsFile%</string>
			<key>bodyName</key>
			<string>%physicsBodyName%</string>
			<key>layers</key>
			<integer>3</integer>
			<key>group</key>
			<string>LEVEL</string>
		</dict>
		<key>render</key>
		<dict>
			<key>sprites</key>
			<array>
				<dict>
					<key>name</key>
					<string>default</string>
					<key>textureFile</key>
					<string>%renderTextureFile%</string>
					<key>z</key>
					<integer>2</integer>
					<key>anchorPoint</key>
					<string>{ 0.5, 0.5 }</string>
				</dict>
			</array>
		</dict>
		<key>shard</key>
		<dict>
			<key>piecesEntityType</key>
			<string>%shardPiecesEntityType%</string>
			<key>piecesCount</key>
			<integer>15</integer>
            <key>piecesSpawnType</key>
            <string>SHARD_PIECES_SPAWN_FADEOUT</string>
		</dict>
		<key>sound</key>
        <dict>
            <key>defaultCollisionSound</key>
			<string>BeeHitGlass</string>
            <key>defaultDestroySound</key>
            <string>GlassLarge</string>
        </dict>
		<key>transform</key>
		<dict/>
	</dict>
</dict>
</plist>
